<Window x:Class="WPF2_6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF2_6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="button" Content="Microsoft"
                Click="button_Click"
                HorizontalAlignment="Left" Margin="266,0,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Intel"
                Click="button1_Click"
                HorizontalAlignment="Left" Margin="266,0,0,0" Grid.Row="1" VerticalAlignment="Center"/>

        <StackPanel Height="208">
            <Frame x:Name="Frame" Margin="65,0,0,5" Visibility="Hidden"
           Source="https://www.microsoft.com/ko-kr/"
             ></Frame>
        </StackPanel>
        <StackPanel>
            <Frame x:Name="Frame2" Margin="125,0,0,5" Visibility="Hidden"
           
             ></Frame>
        </StackPanel>
    </StackPanel>

</Window>
